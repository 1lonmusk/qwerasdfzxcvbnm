else:

if True:
    if False:
        print("A")
    else:
        print("B")
else:
    if True:
        print("C")
    else:
        print("D")
print("E")

a = 3
b = 4
if a >= b:
    print(a)

a = int(input())
b = int(input())

if a > b:
    print("a가 b보다 큽니다")
elif a < b:
    print("a가 b보디 작습니다")
elif a == b:
    print("a와 b가 같습니다")

