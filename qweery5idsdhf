a = 3
b = 4

print(bool(a not b))

a = ("A")
b = ("python")
if a in ["A","B","C"]:
    print(a)

if " y" not in b:
    print("p")
else:
    print(b)

H, M = input().split()
H = int(H)
M = int(M)
timer = int(input("timer: "))
M += timer

if M >= 60:
    H += M // 60
    M = M % 60
    if H >= 24:
        H = H % 24

print(H,M)

n = int(input("n: "))

m = n
cnt = 0

while True:
    part1 = m % 10
    part2 = (m // 10 + m % 10) % 10
    m = part1 * 10 + part2
    cnt += 1
    if m == n:
        break
print(cnt)
