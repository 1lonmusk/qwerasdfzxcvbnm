a = True
b = False
print(type(a))
print(type(b))

print(bool('python'),bool(""))
print(bool({1,2,3}), bool([]))
print(bool((1, 2)), bool (( )))
print(bool({"1" : 1}), bool ({}))
print(bool(-3), bool(0))
print(bool(None))

